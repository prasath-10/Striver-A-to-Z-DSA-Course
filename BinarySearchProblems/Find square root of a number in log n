class Solution {
    int floorSqrt(int n) {
        int start=1;
        int high=n;
        int ans=1;
        while(start<=high)
        {
            int mid=(start+high)/2;
            if(mid*mid<=n)
            {
                ans=mid;
                start=mid+1;
            }
            else{
                high=mid-1;
            }
        }
        return ans;
        
    }
}
